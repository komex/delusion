{
    "name": "komex/delusion",
    "description": "Mock object instrument. Allows to mock and stub all classes.",
    "homepage": "http://komex.github.io/delusion/",
    "keywords": [
        "testing", "test", "unit test", "mock", "stub", "framework", "instrument"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andrey Kolchenko",
            "email": "andrey@kolchenko.me",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "andrey@kolchenko.me",
        "source": "https://github.com/komex/delusion",
        "issues": "https://github.com/komex/delusion/issues"
    },
    "require": {
        "php": ">=5.4.0",
        "andrewsville/php-token-reflection": "1.3.*"
    },
    "require-dev": {
        "komex/unteist": "1.0.*"
    },
    "autoload": {
        "psr-0": {
            "Delusion": "src",
            "Tests\\Delusion": "tests"
        }
    },
    "suggest": {
        "komex/unteist": "A powerful unit test framework."
    }
}